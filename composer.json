{
  "name": "ucarsolutions/vdirsyncerui",
  "description": "Saving configuration data to pass to the external program vdirsyncer",
  "type": "project",
  "license": "AGPL",
  "authors": [
    {
      "name": "Ucar Solutions UG",
      "email": "info@ucar-solutions.de"
    }
  ],
  "require-dev": {
    "phpunit/phpunit": "^10.0.14",
    "symfony/var-dumper": "^5.4",
    "phpcompatibility/php-compatibility": "^9.3"
  },
  "require": {
    "doganoo/di-services": "^0.0.43"
  },
  "scripts": {
    "add-php-compatibility": "\"vendor/bin/phpcs\" --config-set installed_paths vendor/phpcompatibility/php-compatibility",
    "php-compatibility": "./vendor/bin/phpcs -p . --standard=PHPCompatibility --runtime-set testVersion 7.4-8.2 --extensions=php --ignore=vendor/"
  }
}
